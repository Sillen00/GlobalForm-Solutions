{
  "name": "globalform-solutions",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "next build",
    "db:update": "prisma db push && prisma generate",
    "db:studio": "prisma studio",
    "db:seed": "node --loader ts-node/esm prisma/seed.ts",
    "dev": "prisma db push && next dev",
    "postinstall": "prisma generate && husky install",
    "lint": "next lint",
    "start": "next start",
    "format": "prettier --write \"**/*.{ts,tsx}\" && npx prisma format",
    "format:check": "prettier --check \"**/*.{ts,tsx}\"",
    "prepare": "husky install"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --write",
      "next lint"
    ]
  },
  "dependencies": {
    "@clerk/nextjs": "^4.29.0",
    "@clerk/themes": "^1.7.9",
    "@prisma/client": "^5.6.0",
    "@t3-oss/env-nextjs": "^0.7.1",
    "@tanstack/react-query": "^4.36.1",
    "@trpc/client": "^10.43.6",
    "@trpc/next": "^10.43.6",
    "@trpc/react-query": "^10.43.6",
    "@trpc/server": "^10.43.6",
    "localtunnel": "^2.0.2",
    "next": "^14.0.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-icons": "^4.12.0",
    "server-only": "^0.0.1",
    "superjson": "^2.2.1",
    "svix": "^1.15.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@clerk/clerk-sdk-node": "^4.13.4",
    "@clerk/types": "^3.60.0",
    "@next/eslint-plugin-next": "^14.0.3",
    "@types/eslint": "^8.44.7",
    "@types/node": "^18.19.3",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@typescript-eslint/eslint-plugin": "^6.11.0",
    "@typescript-eslint/parser": "^6.11.0",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.54.0",
    "husky": "^8.0.0",
    "lint-staged": "^15.2.0",
    "postcss": "^8.4.31",
    "prettier": "^3.1.0",
    "prettier-plugin-tailwindcss": "^0.5.7",
    "prisma": "^5.6.0",
    "sass": "^1.69.5",
    "tailwindcss": "^3.3.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "ct3aMetadata": {
    "initVersion": "7.25.0"
  },
  "packageManager": "npm@10.2.3"
}
